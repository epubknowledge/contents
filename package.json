{
  "name": "@epubknowledge/contents",
  "version": "0.1.2",
  "description": "NPM Package that gets ePub contents from a passed directory and returns as object",
  "main": "bin/index.js",
  "bin": {
    "ekContents": "bin/index.js",
    "contents": "bin/index.js"
  },
  "publishConfig": {
    "access": "restricted"
  },
  "files": [
    "bin/",
    "src/"
  ],
  "engines": {
    "node": ">=14.0"
  },
  "os": [
    "darwin",
    "linux",
    "!win32"
  ],
  "scripts": {
    "lint": "eslint",
    "test": "jest",
    "coverage": "jest --coverage"
  },
  "jest": {
    "verbose": false,
    "roots": [
      "__tests__",
      "bin",
      "src"
    ],
    "modulePathIgnorePatterns": [
      "<rootDir>/__tests__/setup"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/epubknowledge/contents.git"
  },
  "keywords": [
    "epub",
    "contents"
  ],
  "author": "codingChewie",
  "license": "see license in LICENSE",
  "bugs": {
    "url": "https://github.com/epubknowledge/contents/issues"
  },
  "homepage": "https://github.com/epubknowledge/contents#readme",
  "dependencies": {
    "chalk": "^4.1.1"
  },
  "devDependencies": {
    "eslint": "^7.29.0",
    "jest": "^27.0.5",
    "prettier": "^2.3.1"
  }
}
